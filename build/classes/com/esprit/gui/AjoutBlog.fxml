<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="530.0" prefWidth="819.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esprit.gui.AjoutBlogController">
   <children>
      <Accordion />
      <Button fx:id="btnModifier" layoutX="112.0" layoutY="284.0" mnemonicParsing="false" onAction="#Modifierblog" prefHeight="30.0" prefWidth="76.0" text="Modifier">
         <font>
            <Font size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnSupprimer" layoutX="215.0" layoutY="282.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="85.0" text="Supprimer">
         <font>
            <Font size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnAjouter" graphicTextGap="10.0" layoutX="14.0" layoutY="283.0" mnemonicParsing="false" onAction="#blogadd1" prefHeight="31.0" prefWidth="76.0" text="Ajouter ">
         <font>
            <Font size="12.0" />
         </font>
      </Button>
      <VBox layoutX="9.0" layoutY="80.0" prefHeight="154.0" prefWidth="207.0" spacing="15.0">
         <children>
            <Label text="Titre " />
            <Label text="Description" />
            <Label text="idUtilisateur " />
            <Label text="idBlog" />
         </children>
      </VBox>
      <VBox layoutX="101.0" layoutY="75.0" prefHeight="116.0" prefWidth="199.0" spacing="10.0">
         <children>
            <TextField fx:id="tfTitre" prefHeight="31.0" prefWidth="158.0" />
            <TextField fx:id="tfDescription" prefHeight="72.0" prefWidth="122.0" />
            <TextField fx:id="tfidUtilisateur" prefHeight="72.0" prefWidth="122.0" />
            <TextField fx:id="tfidBlog" prefHeight="72.0" prefWidth="122.0" />
         </children>
      </VBox>
      <TableView fx:id="information" layoutX="323.0" layoutY="80.0" onMouseClicked="#getSelected" prefHeight="232.0" prefWidth="465.0">
        <columns>
          <TableColumn fx:id="colidblog" prefWidth="75.0" text="idBlog" />
          <TableColumn fx:id="colidutilisateur" prefWidth="115.0" text="idUtilisateur" />
            <TableColumn fx:id="cotitre" prefWidth="75.0" text="Titre " />
            <TableColumn fx:id="coldesc" prefWidth="97.0" text="Description" />
        </columns>
      </TableView>
      <Button fx:id="btnAfficherleblog" graphicTextGap="10.0" layoutX="112.0" layoutY="339.0" mnemonicParsing="false" onAction="#afficherleblog" prefHeight="31.0" prefWidth="76.0" text="Afficher">
         <font>
            <Font size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
